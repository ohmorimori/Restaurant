<div id="map"></div>
<div class="main shops-index">
  <div class="container">
    <% if @result["rest"].present? %>
      <% @result["rest"].each do |shop| %>
        <div class="shops-index-item">
  	      <div class="shop-name">
  	        <%= link_to(shop["name"], shop["url"], target: "_blank") %>
            <%= shop["latitude"] %>
            <%= shop["longitude"] %>
  	      </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>